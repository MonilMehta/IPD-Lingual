{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./typings"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "assets/*": ["./assets/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/index.tsx", "app/_layout.jsx", "app/(main)/settings.tsx", "app/(main)/profile.tsx", "app/(main)/home.tsx", "app/(main)/camera.tsx", "screens/CameraScreen.js"  ]
}
